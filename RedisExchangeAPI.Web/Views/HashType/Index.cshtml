@model Dictionary<string,string>
@{
    ViewData["Title"] = "Index";
}
<div class="align-content-center justify-content-center">
    <br />
    <a asp-action="DeleteFirstItem" style="background-color:red">Baştan Sil</a>
    <br />
    <form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
        <input type="text" name="name" />
        <input type="text" name="val" />
        <button type="submit">Kaydet</button>
    </form>
</div>


@foreach (var item in Model)
{


    <h2>@item.Key -> @item.Value <a asp-controller="ListType" asp-action="DeleteItem" asp-route-name="@item.Key">Sil </a></h2>
}
